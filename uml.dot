digraph G {
    node [shape=record]
    Graph [label="{Graph|+ nodes : List[Node]\l+ edges : List[Edge]\l|+ add_node(value : int) : void\l+ add_edge(value1 : int, value2 : int) : void\l+ find_node(value : int) : Optional[Node]\l}"]
    Node [label="{Node|+ value : int\l|}"]
    Edge [label="{Edge|+ node1 : Node\l+ node2 : Node\l|}"]
    GraphAgent [label="{GraphAgent|+ start : Any\l+ target : Any\l+ graph : nx.Graph\l+ shortest_paths : Dict[Any, Dict[Any, List[Any]]]\l+ current_node : Any\l+ memory : List[Any]\l|+ random_walk() : void\l+ shortest_path() : void\l}"]
    Node -> Edge [label="node1"]
    Node -> Edge [label="node2"]
    Node -> Graph [label="nodes"]
    Edge -> Graph [label="edges"]
    Graph -> GraphAgent [label="graph"]
}